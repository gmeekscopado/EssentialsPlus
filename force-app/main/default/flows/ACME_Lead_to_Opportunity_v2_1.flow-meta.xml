    <decisions>
        <name>Basic_Qualification_Check</name>
        <label>Basic Qualification Check</label>
        <locationX>176</locationX>
        <locationY>242</locationY>
        <defaultConnectorLabel>Not Qualified</defaultConnectorLabel>
        <rules>
            <name>Meets_Basic_Criteria</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Email</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Company</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Working - Contacted</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Initialize_Score</targetReference>
            </connector>
            <label>Meets Basic Criteria</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_Company_Size</name>
        <label>Check Company Size</label>
        <locationX>176</locationX>
        <locationY>642</locationY>
        <defaultConnector>
            <targetReference>Check_Industry</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Small Company</defaultConnectorLabel>
        <rules>
            <name>Large_Company</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.NumberOfEmployees</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>500.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.AnnualRevenue</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>1.0E7</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_Company_Score</targetReference>
            </connector>
            <label>Large Company</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_Email_Domain</name>
        <label>Check Email Domain</label>
        <locationX>176</locationX>
        <locationY>402</locationY>
        <defaultConnector>
            <targetReference>Check_Title_Seniority</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Personal Email</defaultConnectorLabel>
        <rules>
            <name>Corporate_Email</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Email</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Email</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>@</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_Email_Score</targetReference>
            </connector>
            <label>Corporate Email</label>
        </rules>
    </decisions>
    <decisions>
