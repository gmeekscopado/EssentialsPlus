    <decisions>
        <name>Determine_Priority_Level</name>
        <label>Determine Priority Level</label>
        <locationX>176</locationX>
        <locationY>1190</locationY>
        <defaultConnector>
            <targetReference>Set_Standard_Priority</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Standard</defaultConnectorLabel>
        <rules>
            <name>Hot_Lead</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_LeadScore</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>70.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Hot_Priority</targetReference>
            </connector>
            <label>Hot Lead (70+ points)</label>
        </rules>
        <rules>
            <name>Warm_Lead</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_LeadScore</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>50.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_LeadScore</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <numberValue>70.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Warm_Priority</targetReference>
            </connector>
            <label>Warm Lead (50-69 points)</label>
        </rules>
    </decisions>
